  FrameCreator in 'FrameCreator.pas',
  FileCreator in 'FileCreator.pas',
  logger in 'logger.pas',
  toolBox in 'toolBox.pas',
  copyfileform in 'copyfileform.pas' {FormCopyFile},
  shadow in 'shadow.pas',
  ViewCreator in 'ViewCreator.pas',
  ControllerCreator in 'ControllerCreator.pas',
  ModelCreator in 'ModelCreator.pas',
  ProjectFileCreator in 'ProjectFileCreator.pas',
  AppWizardForm in 'AppWizardForm.pas' {FormAppWizard},
  NewProjectWizard in 'NewProjectWizard.pas',
  NewSetForm in 'NewSetForm.pas' {FormNewSet},
  Patterns in 'Patterns.pas',
  OTAUtilities in 'OTAUtilities.pas';

end.

